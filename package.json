{
  "name": "AI-paste",
  "displayName": "AI-paste",
  "description": "快速粘贴AI生成的文件内容，自动生成对应文件夹",
  "version": "1.0.0",
  "publisher": "zgarry",
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [
    "onCommand:copyToFile.createFile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copyToFile.createFile",
        "title": "Create File from Clipboard"
      }
    ],
    "keybindings": [
      {
        "command": "copyToFile.createFile",
        "key": "ctrl+shift+v",
        "mac": "cmd+shift+v"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.50.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.10.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.3",
    "vscode-test": "^1.4.0"
  }
}